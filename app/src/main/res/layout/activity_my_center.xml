<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
                xmlns:tools="http://schemas.android.com/tools"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:paddingLeft="@dimen/activity_horizontal_margin"
                android:paddingRight="@dimen/activity_horizontal_margin"
                android:paddingTop="@dimen/activity_vertical_margin"
                android:paddingBottom="@dimen/activity_vertical_margin"
                tools:context="phitlab.org.lab2.MyCenter">

    <ScrollView
        android:layout_width="fill_parent"
        android:layout_height="475dp"
        android:id="@+id/scrollView3">
        <LinearLayout
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="fill_parent" android:id="@+id/linearLayout2">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceLarge"
                android:text="My Center"
                android:id="@+id/textView11"/>
            <TableRow
                android:layout_width="fill_parent"
                android:layout_height="wrap_content">
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textAppearance="?android:attr/textAppearanceLarge"
                    android:text="I&apos;m in"
                    android:id="@+id/textView5"
                    android:layout_marginRight="135dp" android:layout_marginLeft="5dp"
                    android:layout_marginTop="5dp" android:layout_marginBottom="5dp"/>
                <Button
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="View History"
                    android:id="@+id/event_history"/>
            </TableRow>
            <GridLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content">
                <ImageView
                    android:layout_width="103dp"
                    android:layout_height="103dp"
                    android:id="@+id/imageView" android:layout_row="0" android:layout_column="0"
                    android:src="@drawable/baseball3" android:layout_margin="3dp"
                    android:layout_columnSpan="2"/>
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Event 1"
                    android:id="@+id/textView6" android:layout_row="0" android:layout_column="1"/>
                <ImageView
                    android:layout_width="103dp"
                    android:layout_height="103dp"
                    android:id="@+id/imageView2" android:layout_row="0" android:layout_column="2"
                    android:src="@drawable/basketball" android:layout_margin="3dp"
                    android:layout_columnSpan="2"/>
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Event 2"
                    android:id="@+id/textView7" android:layout_row="0" android:layout_column="3"/>
                <ImageView
                    android:layout_width="103dp"
                    android:layout_height="103dp"
                    android:id="@+id/imageView3" android:layout_row="1" android:layout_column="0"
                    android:src="@drawable/baseball1" android:layout_margin="3dp"/>
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Event 3"
                    android:id="@+id/textView8" android:layout_row="1" android:layout_column="1"/>
                <ImageView
                    android:layout_width="103dp"
                    android:layout_height="103dp"
                    android:id="@+id/imageView4" android:layout_row="1" android:layout_column="2"
                    android:src="@drawable/swim1" android:layout_margin="3dp"/>
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Event 4"
                    android:id="@+id/textView9" android:layout_row="1" android:layout_column="3"/>
            </GridLayout>
            <TableRow
                android:layout_width="match_parent"
                android:layout_height="wrap_content" android:weightSum="1">
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textAppearance="?android:attr/textAppearanceLarge"
                    android:text="I&apos;m interested in"
                    android:id="@+id/textView10"
                    android:layout_marginLeft="5dp" android:layout_marginTop="10dp"
                    android:layout_marginRight="50dp" android:layout_marginBottom="5dp"/>
                <Button
                    style="?android:attr/buttonStyleSmall"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="View More"
                    android:id="@+id/event_more"/>
            </TableRow>
            <GridLayout
                android:layout_width="fill_parent"
                android:layout_height="147dp">
                <ImageView
                    android:layout_width="70dp"
                    android:layout_height="70dp"
                    android:id="@+id/imageView5" android:layout_row="0" android:layout_column="0"
                    android:src="@drawable/baseball2"/>
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Event Name"
                    android:id="@+id/textView14" android:layout_row="1" android:layout_column="0"/>
                <ImageView
                    android:layout_width="70dp"
                    android:layout_height="70dp"
                    android:id="@+id/imageView6" android:layout_row="0" android:layout_column="1"
                    android:src="@drawable/baseball1" android:layout_marginLeft="56dp"
                    android:layout_marginRight="53dp"/>
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Event Name"
                    android:id="@+id/textView15" android:layout_row="1" android:layout_column="1"
                    android:layout_gravity="center_horizontal"/>
                <ImageView
                    android:layout_width="70dp"
                    android:layout_height="70dp"
                    android:id="@+id/imageView7" android:layout_row="0" android:layout_column="2"
                    android:src="@drawable/basketball1"/>
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Event Name"
                    android:id="@+id/textView16" android:layout_row="1" android:layout_column="2"/>



                <ImageView
                    android:layout_width="70dp"
                    android:layout_height="70dp"
                    android:id="@+id/imageView8" android:layout_row="2" android:layout_column="0"
                    android:src="#ffbdbcb6"/>
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Event Name"
                    android:id="@+id/textView17" android:layout_row="3" android:layout_column="0"/>
                <ImageView
                    android:layout_width="70dp"
                    android:layout_height="70dp"
                    android:id="@+id/imageView9" android:layout_row="2" android:layout_column="1"
                    android:src="#ffbdbcb6" android:layout_marginLeft="56dp"
                    android:layout_marginRight="53dp"/>
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Event Name"
                    android:id="@+id/textView18" android:layout_row="3" android:layout_column="1"
                    android:layout_gravity="center_horizontal"/>
                <ImageView
                    android:layout_width="70dp"
                    android:layout_height="70dp"
                    android:id="@+id/imageView10" android:layout_row="2" android:layout_column="2"
                    android:src="#ffbdbcb6"/>
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Event Name"
                    android:id="@+id/textView19" android:layout_row="3" android:layout_column="2"/>

            </GridLayout>
        </LinearLayout>
    </ScrollView>

    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentRight="true" android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true" android:id="@+id/linearLayout"
        android:layout_alignParentStart="true">
        <TableLayout
            android:layout_width="fill_parent"
            android:layout_height="53dp" android:layout_gravity="bottom">
            <TableRow
                android:layout_width="match_parent"
                android:layout_height="match_parent" android:background="#39fffdca">
                <Button
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/home"
                    android:id="@+id/home" android:layout_column="0" android:layout_margin="5dp"/>
                <Button
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/my_center"
                    android:id="@+id/my_center" android:layout_column="1"
                    android:layout_margin="5dp"/>
                <Button
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="New Event"
                    android:id="@+id/new_event" android:layout_column="2"
                    android:layout_margin="5dp"/>
            </TableRow>
        </TableLayout>
    </LinearLayout>

</RelativeLayout>
