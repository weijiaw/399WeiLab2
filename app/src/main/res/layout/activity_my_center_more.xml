<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
                xmlns:tools="http://schemas.android.com/tools"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:paddingLeft="@dimen/activity_horizontal_margin"
                android:paddingRight="@dimen/activity_horizontal_margin"
                android:paddingTop="@dimen/activity_vertical_margin"
                android:paddingBottom="@dimen/activity_vertical_margin"
                tools:context="phitlab.org.lab2.MyCenterMore">



    <GridLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content" android:layout_marginLeft="5dp"
        android:layout_marginRight="5dp" android:layout_marginTop="5dp">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:text="Events I&apos;m intersted in"
            android:id="@+id/textView27" android:layout_row="0" android:layout_column="0"/>
        <ImageView
            android:layout_width="100dp"
            android:layout_height="100dp"
            android:id="@+id/imageView11" android:layout_row="1" android:layout_column="0"
            android:src="#ffbdbcb6" android:layout_margin="5dp"/>
        <ImageView
            android:layout_width="100dp"
            android:layout_height="100dp"
            android:id="@+id/imageView12" android:layout_row="2" android:layout_column="0"
            android:src="#ffbdbcb6" android:layout_margin="5dp"/>
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Event 1"
            android:id="@+id/textView28" android:layout_row="1" android:layout_column="1"/>
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Event3"
            android:id="@+id/textView29" android:layout_row="2" android:layout_column="1"/>
        <ImageView
            android:layout_width="100dp"
            android:layout_height="100dp"
            android:id="@+id/imageView13" android:layout_row="1" android:layout_column="2"
            android:src="#ffbdbcb6"/>
        <ImageView
            android:layout_width="100dp"
            android:layout_height="100dp"
            android:id="@+id/imageView14" android:layout_row="2" android:layout_column="2"
            android:layout_margin="5dp" android:src="#ffbdbcb6"/>
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Event 2"
            android:id="@+id/textView30" android:layout_row="1" android:layout_column="3"/>
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Event 4"
            android:id="@+id/textView31" android:layout_row="2" android:layout_column="3"/>





        <ImageView
            android:layout_width="100dp"
            android:layout_height="100dp"
            android:id="@+id/imageView15" android:layout_row="4" android:layout_column="0"
            android:src="#ffbdbcb6" android:layout_margin="5dp"/>
        <ImageView
            android:layout_width="100dp"
            android:layout_height="100dp"
            android:id="@+id/imageView16" android:layout_row="4" android:layout_column="0"
            android:src="#ffbdbcb6" android:layout_margin="5dp"/>
        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/imageView19" android:layout_row="3" android:layout_column="0"
            android:src="#ffbdbcb6"/>
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Event 5"
            android:id="@+id/textView33" android:layout_row="4" android:layout_column="1"/>
        <ImageView
            android:layout_width="100dp"
            android:layout_height="100dp"
            android:id="@+id/imageView18" android:layout_row="4" android:layout_column="2"
            android:layout_margin="5dp" android:src="#ffbdbcb6"/>
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Event 6"
            android:id="@+id/textView35" android:layout_row="4" android:layout_column="3"/>
        <ImageView
            android:layout_width="100dp"
            android:layout_height="100dp"
            android:id="@+id/imageView17" android:layout_row="5" android:layout_column="0"
            android:src="#ffbdbcb6" android:layout_margin="5dp"/>
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Event 7"
            android:id="@+id/textView32" android:layout_row="5" android:layout_column="1"/>
        <ImageView
            android:layout_width="100dp"
            android:layout_height="100dp"
            android:id="@+id/imageView20" android:layout_row="5" android:layout_column="2"
            android:src="#ffbdbcb6" android:layout_margin="5dp"/>
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Event 8"
            android:id="@+id/textView34" android:layout_row="5" android:layout_column="3"/>
    </GridLayout>


    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentRight="true" android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true" android:id="@+id/linearLayout"
        android:layout_alignParentStart="true">
        <TableLayout
            android:layout_width="fill_parent"
            android:layout_height="53dp" android:layout_gravity="bottom">
            <TableRow
                android:layout_width="match_parent"
                android:layout_height="match_parent" android:background="#39fffdca">
                <Button
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/home"
                    android:id="@+id/home" android:layout_column="0" android:layout_margin="5dp"/>
                <Button
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/my_center"
                    android:id="@+id/my_center" android:layout_column="1"
                    android:layout_margin="5dp"/>
                <Button
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="New Event"
                    android:id="@+id/new_event" android:layout_column="2"
                    android:layout_margin="5dp"/>
            </TableRow>
        </TableLayout>
    </LinearLayout>

</RelativeLayout>
